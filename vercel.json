{
  "version": 2,
  "builds": [
    {
      "src": "packages/web-ui/dist/**/*",
      "use": "@vercel/static"
    }
  ],
 "redirects": [
    {
      "source": "/404",
      "destination": "/",
      "statusCode": 307
    },
 	{
      "source": "/:path*",
      "destination": "/",
      "statusCode": 307
    }

  ],

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/packages/web-ui/dist/index.html"
    }
  ]
}

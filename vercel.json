{
  "version": 2,
  "builds": [
    {
      "src": "packages/web-ui/dist/**/*",
      "use": "@vercel/static"
    }
  ],
"redirects": [
    {
      "source": "/:path*", // Matches all paths
      "destination": "/", // Redirect to the homepage
      "statusCode": 307
    }
  ]
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/packages/web-ui/dist/index.html"
    }
  ]
}
